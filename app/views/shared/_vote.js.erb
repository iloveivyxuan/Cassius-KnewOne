<% id = "#{object.model_name.singular}_#{object.id}" %>
$('#<%= id %> .voting')
.replaceWith("<%=j render 'shared/voting', object: object %>")
$element = $('#<%= id %> .voting');
if ($element.find('.btn--like').hasClass('done')) {
  $element.find('.fa')
    .on(Making.prefixEvent('AnimationEnd'), function(event) {
      $(this).removeClass('heartbeat');
    }).addClass('heartbeat');
}
